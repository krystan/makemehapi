{
  "connections" : [{
    "port": 3000,
    "labels": ["api"],
    "routes": {"cors": true}}],
  "registrations": [
    {
      "plugin": {
        "register": "good",
        "options": {
          "reporters": [{
            "reporter": "good-console",
            "events": {
              "response": "*",
              "log": "*"
            }
          }]
        }
      }
    },
    {
      "plugin": {
        "register": "inert",
        "options": {"select": "api", "options": {}}
      }
    },
    {
      "plugin": {
        "register": "vision",
        "options": {}
      }
    },
    {
      "plugin": {
        "register": "hapi-swagger",
        "options": {"info": {"title": "My Api", "version": "1.0.0"}, "tags": [{"name": "api"}]}
      }
    },
    {
      "plugin": {
        "register": "chairo",
        "options": {}
      }
    },
    {
      "plugin": {
        "register": "./names",
        "options": {}
      }
    }
  ]
}